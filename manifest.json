{
  "manifest_version": 2,

  "name": "CiteMe for Chrome",
  "description": "This extension provides an easy way for students to manage their citations.",
  "version": "1.0",
 
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage",
    "tabs", 
    "<all_urls>"
    ]
}